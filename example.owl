extern fn printf(fmt: *char, ...): i32;

struct Item {
	price: *f64
}

struct Data {
  item: Item
}

fn process(data: Data) {
  *data.item.price = 20.0;
}

fn main(): i32 {
  let data = Data { item: Item { price: &0.0 } };

  process(data);

  printf("value: %f\n\n", *data.item.price);

  return 0;
}

